    "import numpy as np \n",
    "import cv2 \n",
    "  \n",
    "# Open the image. \n",
    "img = cv2.imread('360.jpg') \n",
    "  \n",
    "# Load the mask. \n",
    "mask = cv2.imread('mask1.png', 0) \n",
    "  \n",
    "# Inpaint. \n",
    "dst = cv2.inpaint(img, mask, 3, cv2.INPAINT_NS) \n",
    "  \n",
    "# Write the output. \n",
    "cv2.imwrite('Bike_inpainted.jpg', dst) "
   ]
